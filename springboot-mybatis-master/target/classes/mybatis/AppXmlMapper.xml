<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.no7player.mapper.AppXmlMapper">
  <select id="getAppXml" parameterType="java.util.HashMap">
    select
    *
    from app_xml
    where ID = #{ID}
  </select>
</mapper>